<div class='zasoby-partial'>
  <div
    class='zasoby label opis srodek'
    style='padding-top: 10px;'
  >
    {{localize 'AZS.pc.sheet.resources.ammo.header'}}
  </div>
  <div class='zasobInputDiv'><input
      class='input-zasob prawo'
      name='system.amunicja.value'
      type='text'
      value='{{actor.system.amunicja.value}}'
      data-dtype='Number'
      maxlength='1'
      size='1'
    /></div>
  <div class='srodek' style='padding-top: 4px;'>/</div>
  <div
    class='lewo'
    style='padding-top: 4px;'
  >{{actor.system.amunicja.max}}</div>
  <div class='zasoby label opis srodek'>
    {{localize 'AZS.pc.sheet.resources.armor.header'}}
  </div>
  <div class='zasobInputDiv'><input
      class='input-zasob prawo'
      name='system.pp.value'
      type='text'
      value='{{actor.system.pp.value}}'
      data-dtype='Number'
      maxlength='1'
      size='1'
    /></div>
  <div class='srodek' style='padding-top: 4px;'>/</div>
  <div class='zasobInputDiv'><input
      class='input-zasob-2 lewo'
      name='system.pp.max'
      type='text'
      value='{{actor.system.pp.max}}'
      data-dtype='Number'
      maxlength='1'
      size='1'
    /></div>
  <div class='zasoby label opis srodek'>
    {{localize 'AZS.pc.sheet.resources.torch.header'}}
  </div>
  <div class='zasobInputDiv'>
    <input
      class='input-zasob prawo'
      name='system.pochodnia.value'
      type='text'
      value='{{actor.system.pochodnia.value}}'
      data-dtype='Number'
      maxlength='1'
      size='1'
    />
  </div>
  <div class='srodek' style='padding-top: 4px;'>/</div>
  <div
    class='lewo'
    style='padding-top: 4px;'
  >
    {{actor.system.pochodnia.max}}
  </div>
  <div class='zasoby label opis srodek'>
    {{localize 'AZS.pc.sheet.resources.wound.header'}}
  </div>
  <div class='zasoby label srodek'>
    <label>
      <input
        name='system.rana'
        type='checkbox'
        {{checked actor.system.rana}}
      />
      <span class='czekbox2'>
        <i class='opis fa-regular fa-hand'></i>
      </span>
    </label>
    <input
      class='input-zasob prawo'
      name='system.ranasw.value'
      type='text'
      value='{{actor.system.ranasw.value}}'
      data-dtype='Number'
      maxlength='2'
      size='2'
    />
  </div>
</div>
