<h3>Plecak</h3>
<div class = "plecak_partial">
    <i class = "fa-regular fa-plus"></i>
    <a class="item-create" data-type="bron" title="{{localize "AZS.kartaPostaci.dodajPrzedmiot"}}">
        <i class="fa-regular fa-sword"></i>
    </a>
    <a class="item-create" data-type="zbroja" title="{{localize "AZS.kartaPostaci.dodajPrzedmiot"}}">
        <i class="fa-regular fa-shield"></i>
    </a>
    <a class="item-create" data-type="tom" title="{{localize "AZS.kartaPostaci.dodajPrzedmiot"}}">
        <i class="fa-regular fa-book"></i>
    </a>
    <a class="item-create" data-type="ogolne" title="{{localize "AZS.kartaPostaci.dodajPrzedmiot"}}">
        <i class="fa-regular fa-sack"></i>
    </a>
    <section class = "lista-przedmiotow">
        {{#each itemy as |item|}}
            <div class="item" data-item-id="{{id}}">
                <a class="item-edit">{{name}}</a>
                <a class="item-delete">
                    <i class="fa-regular fa-x"></i>
                </a>
            </div>
        {{/each}}
    </section>
</div>